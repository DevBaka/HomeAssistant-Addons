{
  "name": "DevBaka Addons",
  "url": "https://github.com/DevBaka/HomeAssistant-Addons",
  "maintainer": "DevBaka",
  "addons": [
    {
      "name": "DOCSight",
      "slug": "docsight",
      "description": "Monitor your cable internet connection 24/7 and collect evidence for ISP complaints",
      "version": "1.0.0",
      "arch": ["aarch64", "armv7", "amd64"],
      "startup": "services",
      "boot": "auto",
      "map": ["config:rw"],
      "host_network": true,
      "ports": {
        "8765/tcp": 8765
      },
      "ports_description": {
        "8765/tcp": "DOCSight web interface"
      },
      "options": {
        "log_level": "info"
      },
      "schema": {
        "log_level": "list(debug|info|warning|error)"
      }
    },
    {
      "name": "Icecast Server",
      "slug": "icecast",
      "description": "Icecast2 streaming server for Home Assistant",
      "version": "1.0.0",
      "arch": ["aarch64", "armv7", "amd64"],
      "startup": "services",
      "boot": "auto",
      "map": ["config:rw"],
      "host_network": true,
      "options": {},
      "schema": {}
    }
  ]
}
